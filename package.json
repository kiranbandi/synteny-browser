    "p5": "^0.5.16",
    "paralleljs": "^0.2.1",
    "progressbar.js": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.23.0",
